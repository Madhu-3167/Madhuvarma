package com.BasicJava;

public class ForLoopExample1 {

	public static void main(String[] args) {
		
		/*System.out.println("LiveTech");
		System.out.println("LiveTech");
		System.out.println("LiveTech");
		System.out.println("LiveTech");
		System.out.println("LiveTech");*/
		
		for(int i=1;i<=5;i=i+2)
		{
			//System.out.print(i);
			System.out.println(i+" - "+"LiveTech");
		}
		
		System.out.println("**********************");
		
		for(int k=6;k>=1;k=k-2)
		{
			System.out.println(k+" - "+" Testing ");
		}
		
		
		System.out.println("#####################");
		
		for(int p=7;p<12;p++)
		{
			System.out.println(p+"Selenium");
		}

	}

}
