package com.BasicJava;

public class WhileExample {

	public static void main(String[] args) {
		
		int i=3;
		
		while(i<=5)
		{
			System.out.println(i+" Selenium");
			i++;
		}
		
		System.out.println("-------------------------");
		
		int k=3;
		
		while(k>=-1)
		{
			System.out.println(k+" - "+ " Testing ");
			k--;
		}

	}

}
