package com.BasicJava;

public class VariableExample {

	public static void main(String args[])
	{
		// data_Type variable=value;
		
		int a=10;
		a=20;
		
		String b="LiveTech";
		b="20"; // its considered as String Type - since its placed in " "
		
		double c=10.123;
		
		char d='P';
		
		float e=10;
			
		System.out.println(" The value of a variable is : "+a);
		System.out.println(" The value of b variable is : "+b);
		System.out.println(" The value of c variable is : "+c);
		System.out.println(" The value of d variable is : "+d);
		System.out.println(" The value of e variable is : "+e);
	}
}
