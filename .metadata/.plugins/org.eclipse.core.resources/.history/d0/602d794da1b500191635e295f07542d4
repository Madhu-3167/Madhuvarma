package com.BasicJava;

public class ObjectArrayExample {

	public static void main(String[] args) {
		
		// Object array can store different dataType values
		
		Object array1[] = new Object[3];
		
		array1[0] = 10;
		array1[1] = "LiveTech";
		array1[2] = 10.1234;
		
		for(int i=0;i<3;i++){
		
		System.out.println(array1[i]);
		}
		
		
		
		

	}

}
